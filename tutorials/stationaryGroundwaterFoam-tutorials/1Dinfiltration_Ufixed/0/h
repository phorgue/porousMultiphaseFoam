FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../parameters.txt"

dimensions      [0 1 0 0 0 0 0];

internalField   uniform $param_hinit;

boundaryField
{
    top
    {
        type            darcyGradPressure;
        value           uniform 0; 
    }
    bottom
    {
        type            fixedValue;
        value           uniform $param_houtlet;
    }
    frontAndBack
    {
        type            empty;
    }
}


