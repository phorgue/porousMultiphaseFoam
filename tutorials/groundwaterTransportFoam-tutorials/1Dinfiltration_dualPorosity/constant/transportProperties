//- tracer transport properties
Dm Dm [0 2 -1 0 0 0 0] 5.8e-10;

porousTransport
{
    phaseName theta;
    Kd [-1 3 0 0 0 0 0] 1e-3;
    rs [1 -3 0 0 0 0 0] 0;
