{
    //- fracture properties
    thetaFracturemin 0;
    thetaFracturemax 0.025;
    alphaFracture 5.0; 
    mFracture 0.5;
    //- matrix properties
    thetaMatrixmin 0.10;
    thetaMatrixmax 0.475;
    alphaMatrix 0.5;
    mMatrix 0.333333;
