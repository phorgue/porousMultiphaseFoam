FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         Gauss linear;
}

laplacianSchemes
{
    default         none;
    laplacian(transmissivity,potential) Gauss linear corrected;
}

interpolationSchemes
{
    default		none;
    K			harmonic;
    flux(U)   		linear;
    interpolate(hwater) linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p           ;
}


