FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform -10;

boundaryField
{
    inlet
    {
        type            darcyGradPressure;
        value           uniform -0.75;
        gradient        uniform -0.75;
    }
    outlet
    {
        type            fixedValue;
        value           uniform -10;
    }
    frontAndBack
    {
        type            empty;
    }
}


