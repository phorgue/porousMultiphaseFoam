FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "constant";
  object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Ss Ss [0 -1 0 0 0 0 0] 0;

phase.theta
{
  rho	rho [1 -3 0 0 0 0 0] 	1e3;
  mu	mu [1 -1 -1 0 0 0 0]	1e-3;
}

porousMediumModel dualPorosity;

dualPorosityCoeffs
{
    a 0.01;
    beta 3;
    gammaW 0.4;
    Kmatrix 1.1798e-14; // 1 cm/day
    Kexchange 1.17982e-16; // 0.01 cm/day
}

relativePermeabilityModel  VanGenuchten;

capillarityModel	VanGenuchten;

VanGenuchtenCoeffs
{
    //- fracture properties
    thetaFracturemin 0;
    thetaFracturemax 0.025;
    alphaFracture 5.0; 
    mFracture 0.5;
    //- matrix properties
    thetaMatrixmin 0.10;
    thetaMatrixmax 0.475;
    alphaMatrix 0.5;
    mMatrix 0.333333;
}

