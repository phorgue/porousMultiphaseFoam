FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        system;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint; 

setFormat gnuplot;

surfaceFormat   vtk;

sets
(
    acrossFlow
    {
        type    midPoint;
        axis    y;
        start   ( 0.05 0.60 0.05 );
        end     ( 0.05 0 0.05 );
    }
);

surfaces        ();

fields          ( h );

