FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Utheta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    X0
    {
        type            zeroGradient;
    }

    X1
    {
        type            zeroGradient;
    }

    Y0
    {
        type            zeroGradient;
    }

    Y1
    {
        type            zeroGradient;
    }

    Z0
    {
        type            zeroGradient;
    }

    Z1
    {
        type            zeroGradient;
    }

}


