FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     impesFoam;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         1000;

deltaT          1e-5;

writeControl    adjustableRunTime;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression no;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

CFL             Todd;

maxCo		1;

prefactor       0.5;

dSmax           1;

maxDeltaT	100000;

