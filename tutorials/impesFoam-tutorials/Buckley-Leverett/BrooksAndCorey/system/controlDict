FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     impesFoam;

startFrom       startTime;

startTime       0.0;

stopAt          endTime;

endTime         20000;

deltaT          1e-5;

writeControl    adjustableRunTime;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

CFL            Coats;

maxCo		0.75;
//maxCo         0.01;

dSmax           1.;
//dSmax           0.1;

maxDeltaT	100;

