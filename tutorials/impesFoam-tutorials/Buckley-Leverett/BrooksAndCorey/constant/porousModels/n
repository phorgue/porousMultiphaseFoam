FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      n;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 3;

boundaryField
{

    inlet      
    {
        type            zeroGradient;
    }

    outlet      
    {
	type		zeroGradient;
    }

    frontAndBack    
    {
        type            empty;
    }
}

