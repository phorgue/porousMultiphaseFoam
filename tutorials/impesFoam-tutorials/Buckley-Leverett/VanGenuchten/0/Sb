FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Sb;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2e-4;

boundaryField
{

    inlet      
    {
        type            zeroGradient;
    }

    outlet      
    {
         type            zeroGradient;
    }

    frontAndBack    
    {
        type            empty;
    }
}

