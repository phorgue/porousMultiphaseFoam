FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    inlet      
    {
	type		darcyGradPressure;
	value		uniform 0;
    }

    outlet      
    {
        type	        fixedValue;
	value		uniform 0;
    }

    frontAndBack    
    {
        type            empty;
    }
}

