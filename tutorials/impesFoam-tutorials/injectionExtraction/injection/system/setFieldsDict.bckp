FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
  	   volScalarFieldValue Sb 0.001
	   volScalarFieldValue Sa 0.999
);



regions
(

   boxToCell
   {
     box (0 1 -1) (10 2 1);
     fieldValues
	(
  	   volScalarFieldValue Sb 0.50
	   volScalarFieldValue Sa 0.50
     	);
   }
   
   boxToCell
   {
     box (0 0 -1) (10 1 1);
     fieldValues
	(
  	   volScalarFieldValue Sb 0.999
	   volScalarFieldValue Sa 0.001
     	);
   }
   
);




