FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     porousScalarTransportFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         360000;

deltaT          1e-5;

writeControl    adjustableRunTime;

writeInterval   36000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

adjustTimeStep yes;

runTimeModifiable true;

truncationError 0.01;

maxDeltaT       1e+9;

CSVoutput       true;

eventTimeTracking false;

