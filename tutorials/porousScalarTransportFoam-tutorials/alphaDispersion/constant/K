FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions     [0  2  0 0 0 0 0];

internalField   uniform 1e-11;

boundaryField
{
    x0
    {
        type            zeroGradient;
    }
    
    x1
    {
        type            zeroGradient;
    }
    
    ydirection
    {
        type            zeroGradient;
    }
    
    frontAndBack
    {
        type            empty;
    }
}


