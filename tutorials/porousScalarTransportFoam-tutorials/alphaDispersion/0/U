FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1e-3 1e-4 0);

boundaryField
{
    
    xandy
    {
        type            zeroGradient;
    }

    zdirection
    {
        type            empty;
    }
}


