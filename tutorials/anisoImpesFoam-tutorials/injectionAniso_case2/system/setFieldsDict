FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
	volScalarFieldValue Sb 0.002
	volScalarFieldValue Winj 0
	volScalarFieldValue Wext 0
	volScalarFieldValue eps 0.75
);



regions
(
    // injection
   boxToCell
   {
     box (4.9 8 0) (5.1 8.2 1);
     fieldValues
	(
	   volScalarFieldValue Winj 1 
     	);
   }   

   // extraction
    boxToCell
   {
     box (4.9 0.8 0) (5.1 1 1);
     fieldValues
        (
           volScalarFieldValue Wext 1 
        );
   }   
   
   // extraction
   boxToCell
   {
     box (0 0 0) (5 10 1);
     fieldValues
        (
           volScalarFieldValue eps 0.25 
        );
   }  

);



