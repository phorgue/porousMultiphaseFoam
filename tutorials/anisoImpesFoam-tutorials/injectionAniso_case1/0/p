FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 0;
    }
        axis
    {
       	type            darcyGradPressureAniso;
       	value		uniform 0;     
    }	
 
    inlet
    {
	type		darcyGradPressureAniso;
	value		uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


