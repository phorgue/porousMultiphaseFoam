FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
	volScalarFieldValue z0 0
);



regions
(
    // injection
   boxToCell
   {
     box (0 0 0) (100 5 01);
     fieldValues
	(
	   volScalarFieldValue z0 0.2 
     	);
   }   


);



