FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      hwater;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];


internalField   uniform 0.5;

boundaryField
{
    sides
    {
        type            fixedValue;
        value           uniform 0.5;
    }
    frontAndBack
    {
        type            empty;
    }
}


