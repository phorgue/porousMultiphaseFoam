FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     porousScalarTransport2DFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         864000;

deltaT          1;

writeControl    adjustableRunTime;

writeInterval   72000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

adjustTimeStep yes;

runTimeModifiable true;

maxCo           1.0;

maxDeltaT       1e8;

CSVactive       true;

CSVoutput       "CmassBalance.csv";

